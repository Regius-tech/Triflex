{
  "version": 2,
  "builds": [
    {
      "src": "api/positions.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["api/vehicles.json"]
      }
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/positions",
      "dest": "api/positions.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}