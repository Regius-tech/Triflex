{
  "version": 2,
  "builds": [
    {
      "src": "api/positions.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/positions",
      "dest": "/api/positions.js"
    },
    {
      "src": "/vehicles.json",
      "dest": "/public/vehicles.json"
    },
    {
      "src": "/(logo.*\\.png)", 
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
